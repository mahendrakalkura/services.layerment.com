#!/usr/bin/env php
<?php

require_once __DIR__.'/../functions.php';
require_once __DIR__.'/../parameters.php';

require_once __DIR__.'/../vendor/autoload.php';
require_once __DIR__.'/../vendor/Cpanel/Util/Autoload.php';

